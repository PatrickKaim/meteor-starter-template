    "@tailwindcss/forms": "^0.5.3",
    "browser-image-resizer": "^2.4.1",
    "meteor-node-stubs": "^1.2.1",
    "simpl-schema": "^1.13.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-meteor-tracker": "^3.0.0-beta.7",
    "vue-router": "^4.1.6"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
